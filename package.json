    "inversify-express-utils": "^5.2.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.14",
    "npm-run-all": "^4.1.2",
    "reflect-metadata": "^0.1.12",
    "scrypt-async": "^2.0.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
