{
  "name": "planett6",
  "version": "1.0.0",
  "description": "Planet T6 browser game",
  "author": "Ákos Hofmeister, Máté Molnár",
  "private": true,
  "scripts": {
    "dev-app": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start-app": "yarn dev-app",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit:watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --no-single-run",
    "test": "npm-run-all test-app test-server",
    "test-app": "yarn unit",
    "test-app:watch": "yarn unit:watch",
    "test-server": "mocha-webpack server/src/**/*.spec.ts --webpack-config server/webpack.config.test.js",
    "test-server:watch": "mocha-webpack server/src/**/*.spec.ts --webpack-config server/webpack.config.test.js --watch",
    "lint-server": "tslint -c server/tslint.json 'server/src/**/*.ts'",
    "lint-app": "eslint src",
    "lint-app-test": "eslint test",
    "build-app": "webpack --config build/webpack.prod.conf.js",
    "dev-server": "npm-run-all build-server start-server",
    "start-server": "node --inspect -r source-map-support/register server/dist/index.js",
    "start-server-prod": "node server/dist/index.js",
    "build-server": "webpack --config server/webpack.config.js",
    "build-server-prod": "cross-env NODE_ENV=production webpack --config server/webpack.config.js --mode=production",
    "build": "npm-run-all --parallel build-app build-server-prod",
    "start": "npm-run-all --parallel start-server start-app",
    "analyze": "npm-run-all --parallel lint-server lint-app lint-app-test"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "babel-polyfill": "^6.26.0",
